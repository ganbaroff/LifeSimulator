{
  "name": "lifesimulator-stable",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --testPathPattern=ui",
    "test:components": "jest --testPathPattern=components",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "detox test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run format:check && npm run type-check && npm run test",
    "clean": "expo start --clear",
    "reset": "expo start --reset-cache",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "prebuild": "npm run validate",
    "postinstall": "node node_modules/expo/bin/postinstall"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.8.7",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/native-stack": "^7.8.1",
    "@react-navigation/stack": "^7.6.8",
    "@reduxjs/toolkit": "^2.11.0",
    "@types/axios": "^0.9.36",
    "@types/react-redux": "^7.1.34",
    "@types/redux": "^3.6.31",
    "axios": "^1.13.2",
    "babel-preset-expo": "^54.0.7",
    "crypto-js": "^4.2.0",
    "expo": "^54.0.25",
    "expo-av": "^16.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-localization": "^17.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-adapty": "^3.11.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-mobile-ads": "^16.0.0",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "jest": "^29.7.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
